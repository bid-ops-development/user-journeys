{
  "compilerOptions": {
    "target": "es2019",
    "lib": [
      "dom",
      "dom.iterable",
      "es2019",
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    // "lib": ["ES2019"],
    // "module": "commonjs",
    // "target": "ES2019",

    // "paths": {
    // },
    // "resolveJsonModule": true,
    "isolatedModules": false,
    "noEmit": true,
    // "jsx": "preserve"
  },
  "include": [
    "features"
  ]
}
